{
  "name": "integer_net/solr",
  "type": "magento-module",
  "description": "integer_net Solr Module",
  "homepage": "https://github.com/integer-net/Solr",
  "require": {
    "aoepeople/aoe_layoutconditions": "dev-master",
    "integer-net/solr-enterprise": "dev-master"
  },
  "require-dev": {
    "mikey179/vfsStream": "~1.3"
  },
  "suggest": {
    "firegento/psr0autoloader": "With this extension you can include the composer autoloader and you don't need to activate the custom autoloader shipped with IntegerNet_Solr"
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:schmengler/Aoe_LayoutConditions.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:integer-net/solr-base.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:integer-net/solr-enterprise.git"
    },
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    }
  ]
}
